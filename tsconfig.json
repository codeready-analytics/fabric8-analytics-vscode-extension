{
  "compilerOptions": {
    "target": "es6",
    "downlevelIteration": true,
    "lib": [
      "es6",
      "DOM",
      "DOM.Iterable"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "out",
    "sourceMap": true,
    "rootDir": ".",
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "ca-lsp-server",
    ".vscode-test",
    "test-resources"
  ]
}